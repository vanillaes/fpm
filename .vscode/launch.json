{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "This",
      "type": "node",
      "request": "launch",
      "runtimeVersion": "14.2.0",
      "program": "${file}"
    },
    {
      "name": "Test",
      "type": "node",
      "request": "launch",
      "runtimeVersion": "14.2.0",
      "cwd": "${workspaceFolder}/test",
      "args": [
        "${file}"
      ]
    },
    {
      "name": "Version",
      "type": "node",
      "request": "launch",
      "runtimeVersion": "14.2.0",
      "cwd": "${workspaceFolder}/dev",
      "program": "${workspaceFolder}/bin/ujpm",
      "args": [
        "--version"
      ]
    },
    {
      "name": "Init",
      "type": "node",
      "request": "launch",
      "runtimeVersion": "14.2.0",
      "cwd": "${workspaceFolder}/dev",
      "program": "${workspaceFolder}/bin/ujpm",
      "args": [
        "init"
      ]
    },
    {
      "name": "Install",
      "type": "node",
      "request": "launch",
      "runtimeVersion": "14.2.0",
      "cwd": "${workspaceFolder}/dev",
      "program": "${workspaceFolder}/bin/ujpm",
      "args": [
        "install",
        "npm:@vanillawc/wc-markdown"
      ]
    },
    {
      "name": "Remove",
      "type": "node",
      "request": "launch",
      "runtimeVersion": "14.2.0",
      "cwd": "${workspaceFolder}/dev",
      "program": "${workspaceFolder}/bin/ujpm",
      "args": [
        "remove",
        "npm:@vanillawc/wc-markdown"
      ]
    }
  ]
}
