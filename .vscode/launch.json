{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "runtimeVersion": "13.7.0",
      "name": "Debug",
      "program": "${file}"
    },
    {
      "type": "node",
      "request": "launch",
      "runtimeVersion": "13.7.0",
      "name": "Test",
      "cwd": "${workspaceFolder}/test",
      "args": [
        "${file}"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "runtimeVersion": "13.7.0",
      "name": "Version",
      "cwd": "${workspaceFolder}/dev",
      "program": "${workspaceFolder}/bin/ujpm",
      "args": [
        "--version"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "runtimeVersion": "13.7.0",
      "name": "Init",
      "cwd": "${workspaceFolder}/dev",
      "program": "${workspaceFolder}/bin/ujpm",
      "args": [
        "init"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "runtimeVersion": "13.7.0",
      "name": "Install",
      "cwd": "${workspaceFolder}/dev",
      "program": "${workspaceFolder}/bin/ujpm",
      "args": [
        "install",
        "npm:@vanillawc/wc-markdown"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "runtimeVersion": "13.7.0",
      "name": "Remove",
      "cwd": "${workspaceFolder}/dev",
      "program": "${workspaceFolder}/bin/ujpm",
      "args": [
        "remove",
        "npm:@vanillawc/wc-markdown"
      ]
    }
  ]
}
